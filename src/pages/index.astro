/* Hide scrollbar for Chrome, Safari and Opera */
::-webkit-scrollbar {
    display: none;
}

/* Hide scrollbar for IE, Edge and Firefox */
* {
    -ms-overflow-style: none;  /* IE and Edge */
    scrollbar-width: none;  /* Firefox */
}

/* Hide scrollbar for Chrome, Safari and Opera */
html::-webkit-scrollbar,
body::-webkit-scrollbar {
    width: 0;
    height: 0;
    display: none;
}

/* Hide scrollbar for Firefox */
html, body {
    scrollbar-width: none;
    -ms-overflow-style: none;  /* IE and Edge */
}

html {
    overflow: -moz-scrollbars-none;
    -webkit-overflow-scrolling: touch;
}

body {
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
}

/* Animaciones de scroll */
.fade-in-up {
    opacity: 0;
    transform: translateY(20px);
    transition: opacity 0.6s ease-out, transform 0.6s ease-out;
}

.fade-in-up.visible {
    opacity: 1;
    transform: translateY(0);
}

/* Retrasos para las animaciones */
.delay-1 {
    transition-delay: 0.2s;
}

.delay-2 {
    transition-delay: 0.4s;
}

.delay-3 {
    transition-delay: 0.6s;
}

/* Animaciones más rápidas para la sección hero */
.hero-section.fade-in-up {
    transition: opacity 0.3s ease-out, transform 0.3s ease-out;
}

.hero-section.delay-1 {
    transition-delay: 0.05s;
}

.hero-section.delay-2 {
    transition-delay: 0.1s;
}

.hero-section.delay-3 {
    transition-delay: 0.15s;
}

button#waitlist {
    width: 280px;
    height: 50px;
    color: #fff;
    font-family: "Inter";
    font-weight: 700;
    font-size: 14px;
    letter-spacing: 0.25px;
    background: linear-gradient(180deg, #683FEA, #7b5db8 99%);
    border-radius: 5px;
    position: relative;
    border: none;
    cursor: pointer;
    text-transform: uppercase;
    box-shadow: 0 4px 15px rgba(104, 63, 234, 0.3);
    transition: transform 0.2s ease, box-shadow 0.2s ease;
}

button#waitlist::after, button#waitlist::before {
    content: "";
    background: conic-gradient(from var(--angle), transparent 60%, #fff);
    position: absolute;
    width: 101%;
    height: 101%;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: -2;
    padding: 1.5px;
    border-radius: 5px;
    animation: spin 2.5s linear infinite;
}

button#waitlist::before {
    filter: blur(0.1rem);
}

@keyframes spin {
    from {
        --angle: 0deg;
    }
    to {
        --angle: 360deg;
    }
}
</style>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        const elements = document.querySelectorAll('.fade-in-up');
        
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('visible');
                }
            });
        }, {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        });

        elements.forEach(element => {
            observer.observe(element);
        });
    });
</script>